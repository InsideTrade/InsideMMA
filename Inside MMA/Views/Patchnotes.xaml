<controls:MetroWindow x:Class="Inside_MMA.Views.Patchnotes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inside_MMA.Views"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        mc:Ignorable="d"
        BorderBrush="{DynamicResource AccentColorBrush}"
        BorderThickness="1"
        Title="Patchnotes" SizeToContent="Height" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock Margin="10" FontSize="14" Name="Text" TextWrapping="WrapWithOverflow"></TextBlock>
    </Grid>
</controls:MetroWindow>
